import datetime
import winsound


def alarm(Timing):
    altime = str(datetime.datetime.now().strptime(Timing, "%I:%M %s"))

    altime = altime[11:-3]
    print(altime)
    Horeal = altime[:2]
    Horeal = int(Horeal)
    Mireal = altime[3:5]
    Mireal = int(Mireal)
    print("Done alarm is set for{Timing}")

    while True:
        if Horeal==datetime.datetime.now().hour:
            if Mireal==datetime.datetime.now().minute:
                print("alarm is running sir")
                winsound.PlaySound('abc',winsound.SND_LOOP)

            elif Mireal<datetime.datetime.now().minute:
             break  

if __name__ == '_main_':
    alarm('5:30pm')


